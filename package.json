{
  "name": "jekyll-preact",
  "version": "1.0.0",
  "description": "A simple boilerplate for preact, webpack and jekyll integration.",
  "main": "bundle.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:prod": "jekyll b && webpack -p --env=prod --progress",
    "serve": "jekyll s & webpack -w & bash -c \"echo -e \\\"\\e[45m Use jobs fg kill %<job#> commands to manage background services started. \\\"\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/allizad/jekyll-webpack.git"
  },
  "keywords": [
    "jekyll",
    "webpack",
    "react",
    "preact",
    "unistore",
    "router"
  ],
  "author": "Andrew Grosser",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dioptre/jekyll-preact/issues"
  },
  "homepage": "https://github.com/dioptre/jekyll-preact",
  "devDependencies": {
    "babel-core": "^6.7.7",
    "babel-loader": "^6.2.4",
    "babel-plugin-async-to-promises": "^1.0.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.6.0"
  },
  "dependencies": {
    "preact": "^8.2.7",
    "preact-async-route": "^2.0.2",
    "preact-router": "^2.6.0",
    "ramda": "^0.25.0",
    "unistore": "^3.0.3"
  },
  "babel": {
    "plugins": [
      [
        "transform-react-jsx",
        {
          "pragma": "h"
        }
      ],
      "transform-object-rest-spread"
    ]
  }
}
